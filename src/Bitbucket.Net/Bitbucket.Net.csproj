<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>Apiiro.Bitbucket.Net</PackageId>
    <Version>1.0.3</Version>
    <Authors>Apiiro</Authors>
    <Company>Apiiro</Company>
    <Product>Bitbucket.Net</Product>
    <Copyright>Copyright 2018 by Luk Vermeulen. All rights reserved.</Copyright>
    <PackageLicenseUrl>https://github.com/lvermeulen/Bitbucket.Net/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/lvermeulen/Bitbucket.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/apiiro/Bitbucket.Net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>bitbucket</PackageTags>
    <PackageIconUrl>https://i.imgur.com/OsDAzyV.png</PackageIconUrl>
    <LangVersion>8</LangVersion>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\Bitbucket.Net.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Flurl.Http.Signed" Version="2.3.2" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.Net.Http" Version="4.3.2" />
  </ItemGroup>

</Project>
